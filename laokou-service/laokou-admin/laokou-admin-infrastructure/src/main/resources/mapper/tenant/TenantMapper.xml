<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laokou.admin.gatewayimpl.database.TenantMapper">

    <select id="getTenantListByLikeNameFilter" resultType="org.laokou.admin.gatewayimpl.database.dataobject.TenantDO">
        select id
             ,`name`
        from boot_sys_tenant
        where del_flag = 0
        <if test="name != null and name != ''">
            and `name` like #{name}
        </if>
        order by id desc
    </select>
</mapper>
