<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laokou.admin.gatewayimpl.database.RoleMenuMapper">
  <sql id="columns">
    `id`,
    creator,
    editor,
    create_time,
    update_time,
    dept_id,
    dept_path,
    tenant_id,
    role_id,
    menu_id
  </sql>
  <insert id="insertOne" useGeneratedKeys="false">
    insert into boot_sys_role_menu(<include refid="columns"/>)
    values (
    #{id},
    #{creator},
    #{editor},
    #{createTime},
    #{updateTime},
    #{deptId},
    #{deptPath},
    #{tenantId},
    #{roleId},
    #{menuId}
    )
  </insert>
  <delete id="deleteByRoleId">
    delete
    from boot_sys_role_menu
    where role_id = #{roleId}
  </delete>
</mapper>
