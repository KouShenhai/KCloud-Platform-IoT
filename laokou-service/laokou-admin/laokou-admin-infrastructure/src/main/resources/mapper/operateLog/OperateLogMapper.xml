<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laokou.admin.operateLog.gatewayimpl.database.OperateLogMapper">

	<select id="selectObjectPage"
			resultType="org.laokou.admin.operateLog.gatewayimpl.database.dataobject.OperateLogDO">
		SELECT *
		from boot_sys_operate_log
		where del_flag = 0
		order by id desc
		limit ${pageQuery.pageSize} offset ${pageQuery.pageIndex}
	</select>

	<select id="selectObjectCount" resultType="long">
		SELECT count(1)
		from boot_sys_operate_log
		where del_flag = 0
	</select>

	<select id="selectVersion" resultType="integer">
		select version
		from boot_sys_operate_log
		where id = #{id}
	</select>

</mapper>
